@{

    var GDNumbers = (IEnumerable<SelectListItem>)ViewData["GDNumbers"];
}

<script src="~/js/app.js/CargoDropOffTicketView.js"></script>

<div class="right_col" role="main">
    <div class="page-title">
        <div class="row">
            <div class="col-md-11">
                <div class="title_left">
                    <h3>Cargo Drop Off Ticket</h3>
                </div>
            </div>
            <div class="col-md-1">
            </div>
        </div>
    </div>
    <div class="clearfix"></div>
    <div class="row">
        <div class="col-md-12 col-sm-12 col-xs-12">
            <div class="x_panel">

                <div class="x_content" id="">

                    <form class="form-horizontal form-label-left" id="CargoRollOverForm">

                        <div class="form-horizontal form-label-left row">

                            <div class="col-md-4 col-sm-4 col-xs-12">
                                <div class="item form-group">
                                    <label class="control-label col-md-6" for="first-name">
                                        GD Number
                                    </label>
                                    <div class="col-md-6">
                                        @Html.DropDownList("GDNumber", GDNumbers, "Select GD Number", new { @class = "form-control select2", @id = "gdnumbers", @onchange = "gdNumber_changed(this.value)" })
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-8 col-sm-4 col-xs-12">
                            </div>

                        </div>






                    </form>



                    <div class="row">
                        <div class="col-md-12 col-sm-12 col-xs-12">
                            <div class="x_panel">
                                <div class="x_content">
                                    <div class="form-group" id="gridContainer">
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>



</div>





<script>
    $('.select2').select2();
</script>

