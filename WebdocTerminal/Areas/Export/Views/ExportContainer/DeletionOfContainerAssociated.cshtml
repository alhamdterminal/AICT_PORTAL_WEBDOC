@{
    var ExportContainer = (IEnumerable<SelectListItem>)ViewData["ExportContainer"];

}

<script src="~/js/app.js/DeletionOfContainerAssociated.js"></script>

<div class="right_col" role="main">
    <div class="page-title">
        <div class="row">
            <div class="col-md-11">
                <div class="title_left">
                    <h3>Delete GD Association</h3>
                </div>
            </div>
            <div class="col-md-1">
            </div>
        </div>
    </div>
    <div class="clearfix"></div>

    <div class="row">
        <div class="col-md-12 col-sm-12 col-xs-12">
            <div class="x_panel">

                <div class="x_content" id="">
                    <form class="form-horizontal form-label-left" id="">

                        <div class="form-horizontal form-label-left row">
                            <div class="col-md-4 col-sm-4 col-xs-12">
                                <div class="item form-group">
                                    <label class="control-label col-md-4 col-sm-4 col-xs-12" for="name">
                                        Container Number
                                    </label>
                                    <div class="col-md-8 col-sm-8 col-xs-12">
                                        @Html.DropDownList("ExportContainerId", ExportContainer, "Select Container", new { @class = "form-control select2", @id = "exportContainer" })

                                    </div>
                                </div>
                            </div>

                        </div>

                    </form>






                </div>


            </div>
        </div>

    </div>
    <div class="clearfix"></div>
    <div class="row">
        <div class="col-md-12 col-sm-12 col-xs-12">
            <div class="x_panel">
                <div class="x_content">
                    <div id="ExportContainerItem"></div>
                    <div class="ln_solid"></div>
                </div>

            </div>
        </div>


    </div>


</div>






<script>
    $('.select2').select2();
</script>