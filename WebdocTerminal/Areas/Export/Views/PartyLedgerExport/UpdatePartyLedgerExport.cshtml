@{
    var partys = (IEnumerable<SelectListItem>)ViewData["partys"];
}




<script src="~/js/app.js/UpdatePartyLedgerExport.js"></script>

<div class="right_col" role="main">
    <div class="page-title">
        <div class="title_left">
            <h3>
                Update Party Ledger
            </h3>
        </div>
        <div class="title_right">
        </div>
    </div>
    <div class="clearfix"></div>


    <div class="row">

        <div class="col-md-12 col-sm-12 col-xs-12">
            <div class="x_panel">

                <div class="x_title">

                    <div class="col-md-4 col-sm-4 col-xs-12">
                        <div class="form-group">
                            <label class="control-label  col-md-4 col-xs-12" for="first-name">
                                Party <span class="required">*</span>
                            </label>
                            <div class="col-md-8 col-xs-12">
                                @Html.DropDownList("prty", partys, "Select Party", new { @class = "form-control select2", @id = "prty", required = "required", @onchange = "callChangefunc()" })
                            </div>
                        </div>
                    </div>
                    <div class="clearfix"></div>
                </div>


                <div class="x_content">
                    <div id="PartyLedgersGrid"></div>

                </div>
            </div>
        </div>

    </div>

</div>
