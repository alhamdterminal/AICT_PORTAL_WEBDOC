@{

    var Destinations = (IEnumerable<SelectListItem>)ViewData["Destinations"];

}

<link rel="stylesheet" href="~/css/viewer.part.bundle.css" />
<script src="~/js/viewer.part.bundle.js"></script>


<script src="~/js/app.js/Exportfullout.js"></script>

<div class="right_col" role="main">
    <div class="page-title x_panel">
        <div class="title_left">
            <h3>
                Empty Full Out
            </h3>
        </div>
        <div class="title_right">
            <div class="col-md-6" style="float: right;">
                <div class="item form-group">

                </div>
            </div>
        </div>
    </div>
    <div class="clearfix"></div>
    <div class="clearfix"></div>

    <div class="row">
        <div class="col-md-12 col-sm-12 col-xs-12">
            <div class="x_panel">

                <div class="x_content" id="">
                    <div class="form-horizontal form-label-left row">


                        <div class="col-md-4 col-sm-4 col-xs-12">
                            <div class="item form-group">
                                <label class="control-label col-md-4 col-sm-4 col-xs-12" for="name">
                                    Destination
                                </label>
                                <div class="col-md-8 col-sm-8 col-xs-12">
                                    @Html.DropDownList("Destinations", Destinations, "Select Destinations", new { @class = "form-control select2", @id = "destinationId" })
                                </div>
                            </div>
                        </div>
                        <div class="col-md-4 col-sm-4 col-xs-12">
                            <div class="control-group">
                                <div class="controls">
                                    <label class="control-label col-md-4 col-sm-4 col-xs-12" for="name">
                                        From:
                                    </label>
                                    <div class="col-md-8 col-sm-8 col-xs-12">
                                        <input type="date" class="form-control" id="fromdate">

                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-4 col-sm-4 col-xs-12">

                            <div class="control-group">
                                <div class="controls">
                                    <label class="control-label col-md-4 col-sm-4 col-xs-12" for="name">
                                        To:
                                    </label>
                                    <div class="col-md-8 col-sm-8 col-xs-12">
                                        <input type="date" class="form-control" id="todate">

                                    </div>
                                </div>
                            </div>
                        </div>



                    </div>
                    <div class="form-horizontal form-label-left row">

                        <div class="col-md-12  col-xs-12">
                            <button type="button" style="float:right;" onclick="printReport()" class="btn btn-primary">Print</button>
                        </div>


                    </div>

                </div>
            </div>
        </div>
    </div>

    <div class="row">

        <div class="col-md-12 col-sm-12 col-xs-12">
            <div class="x_panel">
                <div id="large-indicator"></div>
                <div class="x_content" id="repotdata">

                </div>
            </div>
        </div>

    </div>

</div>



<script>
    $('.select2').select2();
</script>