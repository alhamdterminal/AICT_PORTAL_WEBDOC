

@{

    var VesselExports = (IEnumerable<SelectListItem>)ViewData["VesselExports"];

}

<link rel="stylesheet" href="~/css/viewer.part.bundle.css" />
<script src="~/js/viewer.part.bundle.js"></script>


<script src="~/js/app.js/AnfCargoCheckList.js"></script>

<div class="right_col" role="main">
    <div class="page-title x_panel">
        <div class="title_left">
            <h3>
                ANF Cargo Check List
            </h3>
        </div>

        <div class="title_right">


        </div>


    </div>

    <div class="clearfix"></div>

    <div class="row">


        <div class="col-md-12 col-sm-12 col-xs-12">
            <div class="x_panel">
                <div class="x_content" id="">

                    <div class="col-md-3 col-sm-3 col-xs-12">
                        <div class="item form-group">
                            <label class="control-label col-md-4 col-sm-4 col-xs-12" for="name">
                                Vessel
                            </label>
                            <div class="col-md-8 col-sm-8 col-xs-12">
                                @Html.DropDownList("VesselExport", VesselExports, "Select Vessel ", new { @class = "form-control select2", @id = "vesselexport" })
                            </div>
                        </div>
                    </div>


                    <div class="col-md-3 col-sm-3 col-xs-12">
                        <div class="control-group">
                            <div class="controls">
                                <label class="control-label col-md-4  col-xs-12" for="first-name">
                                    From:
                                </label>
                                <div class="col-md-8  col-xs-12">
                                    <input type="date" class="form-control" id="fromdate">
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-3 col-sm-3 col-xs-12">
                        <div class="control-group">
                            <div class="controls">
                                <label class="control-label col-md-4  col-xs-12" for="first-name">
                                    To:
                                </label>
                                <div class="col-md-8  col-xs-12">
                                    <input type="date" class="form-control" id="todate">
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-3 col-sm-3 col-xs-12">
                        <button type="button" class="btn btn-dark mobBtn" onclick="myFunction()" style="float:right">Submit</button>
                    </div>


                </div>
            </div>
        </div>

    </div>
    <div class="row">

        <div class="col-md-12 col-sm-12 col-xs-12">

            <div class="x_panel">
                <div id="large-indicator"></div>
                <div class="x_content" id="repotdata">

                </div>
            </div>
        </div>

    </div>

</div>


<script>
    $('.select2').select2();
</script>

