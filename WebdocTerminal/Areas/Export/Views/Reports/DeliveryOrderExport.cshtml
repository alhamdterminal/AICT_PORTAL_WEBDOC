<link rel="stylesheet" href="~/css/viewer.part.bundle.css" />
<script src="~/js/viewer.part.bundle.js"></script>
<script src="~/js/app.js/DeliveryOrderExport.js"></script>

<div class="right_col" role="main">
    <div class="page-title x_panel">
        <div class="title_left">
            <h3>
                Delivery Order Export
            </h3>
        </div>
    </div>

    <div class="clearfix"></div>

    <div class="x_panel">

        <div class="row">



            <div class="col-md-4 col-sm-4 col-xs-12">
                <div class="item form-group">
                    <label class="control-label col-md-4 col-sm-3 col-xs-12" for="name">
                        GD Number : <span class="required">*</span>
                    </label>
                    <div class="col-md-8 col-sm-8 col-xs-12">
                        @(Html.DevExtreme()
                                             .Autocomplete()
                                            .ID("GDNumber")
                                            .DataSource(d => d.Mvc()
                                            .Controller("APICalls")
                                            .Area("")
                                            .LoadAction("GetGDNumbers"))
                                            .Placeholder("Type GDNumber...")
                                            .OnValueChanged("gdNumber_changed"))
                    </div>
                </div>
            </div>
            <div class="col-md-4 col-sm-4 col-xs-12"></div>

            <div class="col-md-4 col-sm-4 col-xs-12">
                <button type="button" class="btn btn-dark" onclick="myFunction()" style="float:right">Submit</button>

            </div>


        </div>



        <div class="row">

            <div class="col-md-12">




            </div>
        </div>

    </div>
    <div class="clearfix"></div>

    <div class="row">

        <div class="col-md-12 col-sm-12 col-xs-12">
            <div class="x_panel">
                <div id="large-indicator"></div>
                <div class="x_content" id="repotdata">

                </div>
            </div>
        </div>

    </div>

</div>






