<script src="~/js/app.js/EmptyGatePass.js"></script>


<div class="right_col" role="main">
    <div class="page-title">
        <div class="title_left">
            <h3>
                Empty Gate Pass  Re-Print
            </h3>
        </div>
        <div class="title_right">
        </div>
    </div>
    <div class="clearfix"></div>

    <div class="row">

        <div class="col-md-12 col-sm-12 col-xs-12">
            <div class="x_panel">

                <div class="x_content">

                    <div class="row">
                        <div class="col-md-4 col-sm-4 col-xs-12">
                            <div class="item form-group">
                                <label class="control-label col-md-4 col-sm-3 col-xs-12" for="name">
                                    Container : <span class="required">*</span>
                                </label>
                                <div class="col-md-8 col-sm-8 col-xs-12">
                                    @(Html.DevExtreme()
                                        .Autocomplete()
                                        .ID("IGM")
                                        .DataSource(d => d.Mvc()
                                        .Controller("APICalls")
                                        .Area("")
                                        .LoadAction("GetGateInContainers"))
                                        .Placeholder("Type Container Number...")
                                        .OnValueChanged("container_changed"))
                                </div>
                            </div>
                        </div>

                    </div>


                </div>
            </div>
        </div>

    </div>

    <div class="row">

        <div class="col-md-12 col-sm-12 col-xs-12">
            <div class="x_panel">

                <div class="x_content">
                    <div id="emptyContainersgatePasses"></div>                     
                </div>
            </div>
        </div>


    </div>
    </div>

