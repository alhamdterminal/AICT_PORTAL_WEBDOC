@{
    var ShippingAgents = (IEnumerable<SelectListItem>
    )ViewData["ShippingAgents"];

}
<script src="~/js/app.js/ContainerDestuffCFS.js"></script>

<div class="right_col" role="main">
    <div class="page-title">
        <div class="title_left">
            <h3>Destuff Containers</h3>
        </div>

    </div>
    <div class="clearfix"></div>

    <div class="row">
        <div class="col-md-12">
            <div class="x_panel">
                <div class="x_content">
                    <form class="form-horizontal form-label-left" novalidate="" id="TellySheet">

                        <partial name="~/Areas/Import/Views/Shared/_ContainerOrientation.cshtml" />



                        <div class="container-index">

                        </div>

                        <div class="row">
                            <div class="col-md-4 col-sm-4 col-xs-12">
                                <div class="item form-group">
                                    <label class="control-label col-md-4 col-sm-4 col-xs-12" for="name">
                                        Tally Clerk
                                    </label>
                                    <div class="col-md-8 col-sm-8 col-xs-12">
                                        <input type="text" class="form-control" id="tallyClerk" name="TellyClerk" required>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-4 col-sm-4 col-xs-12">
                                <div class="item form-group">
                                    <label class="control-label col-md-4 col-sm-3 col-xs-12" for="name">
                                        Shipping Agent:
                                    </label>
                                    <div class="col-md-8 col-sm-8 col-xs-12">
                                        @Html.DropDownList("ShippingAgentId", ShippingAgents, "Select Shipping Agent", new { @class = "form-control", @id = "shippingAgents" })
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-4 col-sm-4 col-xs-12">
                                <div class="item form-group">
                                    <label class="control-label col-md-2 col-sm-2 col-xs-2" for="name">
                                        Date
                                    </label>
                                    <fieldset>
                                        <div class="control-group">
                                            <div class="controls">
                                                <div class="col-md-12 col-sm-12 col-xs-12 xdisplay_inputx form-group has-feedback">
                                                    @*<input type="datetime" class="form-control has-feedback-left" id="single_cal1" name="DestuffDate" aria-describedby="inputSuccess2Status" required>
        <span class="fa fa-calendar-o form-control-feedback left" aria-hidden="true"></span>
        <span id="inputSuccess2Status" class="sr-only">(success)</span>*@
                                                    <input type="date" class="form-control" id="destuffdate" name="DestuffDate" >
                                                </div>
                                            </div>
                                        </div>
                                    </fieldset>
                                </div>
                            </div>
                        </div>
                         
                        <div class="row">

                            <div class="col-md-4 col-sm-4 col-xs-12" id="containertpe">
                                <div class="form-group">
                                    <label class="control-label col-md-4 col-sm-4 col-xs-12" for="first-name">
                                        Status# <span class="required">*</span>
                                    </label>
                                    <div class="col-md-8 col-sm-8 col-xs-12">
                                        <select class="item form-control" name="ContainerType" required>
                                            <option value="" selected disabled>Please Select</option>
                                            <option value="FCL">FCL</option>
                                            <option value="LCL Lose Cargo">LCL Lose Cargo</option>
                                            <option value="LCL Vehicle">LCL Vehicle</option>
                                        </select>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-3 col-sm-3 col-xs-12 col-sm-offset-1">
                                <div class="item form-group" id="dayNight">

                                    <div class="radio" style="display: inline-block;">
                                        <label>
                                            <input type="radio" class="flat" name="DayNight" id="genderM" value="Day" checked> Day
                                        </label>
                                    </div>
                                    <div class="radio" style="display: inline-block;">
                                        <label>
                                            <input type="radio" class="flat" name="DayNight" id="genderF" value="Nights"> Night
                                        </label>
                                    </div>
                                </div>
                            </div>
                            @*<div class="col-md-2 col-sm-2 col-sm-offset-6">
                                <div class="form-group pull-right">
                                    <a id="buttn" class="btn btn-success" onclick="emptyoutcontainer()">Empty Out</a>
                                </div>
                            </div>*@
                            <div class="col-md-2 col-sm-2 col-sm-offset-6">
                                <div class="form-group pull-right">
                                    <a id="buttn" class="btn btn-success" onclick="submit()" style="display:none">Print Tally Sheet</a>
                                </div>
                            </div>
                        </div>
                        <div class="ln_solid"></div>
                        @*<div class="row">
            <div class="col-md-12 col-sm-12 col-xs-12">


                <div id="destuffContainer">

                </div>
                <div class="ln_solid col-md-12 col-sm-12 col-xs-12"></div>

                <button class="btn btn-primary" style="float:right;" onclick="destuff()" type="button" id="btnSubmit" disabled>Destuff & Save</button>


            </div>
        </div>*@



                    </form>
                </div>
            </div>
        </div>
    </div>
    <div class="row">

        <div class="col-md-12 col-sm-12 col-xs-12">
            <div class="x_panel">

                <div class="x_content">
                    <div id="destuffContainer"></div>
                    <div class="ln_solid"></div>
                    <div class="form-horizontal form-label-left">
                        <div class="form-group">
                            <div class="col-md-12 col-sm-12 col-xs-12">
                                @*<button class="btn btn-primary" style="float:right;" onclick="destuffsaveData()" type="button" id="btnSubmitSec">Save</button>*@

                                <button class="btn btn-primary" style="float:right;" onclick="destuff()" type="button" id="btnSubmit" disabled>Destuff & Submit</button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

    </div>

    <!-- Modal -->
    <div id="excessRemarkModal" class="modal fade" role="dialog">
        <div class="modal-dialog">

            <!-- Modal content-->
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                    <h4 class="modal-title">Excess Remarks</h4>
                </div>
                <div class="modal-body">
                    <input id="excessRemarkInput" class="form-control" type="text" />
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default" data-dismiss="modal" onclick="addExcessRemark()">Save</button>
                </div>
            </div>

        </div>
    </div>



</div>




