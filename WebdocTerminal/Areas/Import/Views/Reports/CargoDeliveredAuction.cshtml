
@{

    var Agents = (IEnumerable<SelectListItem>)ViewData["Agents"];

}
<link rel="stylesheet" href="~/css/viewer.part.bundle.css" />
<script src="~/js/viewer.part.bundle.js"></script>
<script src="~/js/app.js/CargoDeliveredAuction.js"></script>


<div class="right_col" role="main">
    <div class="page-title x_panel">
        <div class="title_left">
            <h3>
                Cargo Delivered (Auction)
            </h3>
        </div>
    </div>

    <div class="clearfix"></div>
    <div class="row">
        <div class="col-md-12 col-sm-12 col-xs-12">
            <div class="x_panel">
                <div class="x_content" id="">




                    <div class="row">

                        <div class="col-md-5">
                            <div class="form-group">
                                <label class="control-label col-md-2" for="first-name">
                                    Shipping Line
                                </label>
                                <div class="col-md-10 col-sm-10 col-xs-12">
                                    @Html.DropDownList("shippingLineId", Agents, "Select Line", new { @class = "form-control select2", @id = "shippingLine" })
                                </div>
                            </div>
                        </div>
                        <div class="col-md-7">
                            @{Html.RenderPartial("_DateFilter"); }
                        </div>
                    </div>



                </div>
            </div>
        </div>
    </div>


    <div class="clearfix"></div>

    <div class="row">

        <div class="col-md-12 col-sm-12 col-xs-12">
            <div class="x_panel">
                <div id="large-indicator"></div>
                <div class="x_content" id="repotdata">

                </div>
            </div>
        </div>

    </div>


</div>





<script>
    $('.select2').select2();
</script>
