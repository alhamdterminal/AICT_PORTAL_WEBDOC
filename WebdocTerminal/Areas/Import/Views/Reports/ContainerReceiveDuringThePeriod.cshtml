@{
    var agents = (IEnumerable<SelectListItem>)ViewData["Agents"];
}
<link rel="stylesheet" href="~/css/viewer.part.bundle.css" />
<script src="~/js/viewer.part.bundle.js"></script>
<script src="~/js/app.js/ContainerReceiveDuringThePeriod.js"></script>

<div class="right_col" role="main">
    <div class="x_panel">
        <div class="page-title ">
            <div class="title_left">
                <h3>
                    Container Receive During The Period
                </h3>
            </div>
            <div class="title_right">
                <div class="row">
                    <div class="col-sm-4" style="float:right;">
                        <select class="form-control" id="type">
                            <option value="0">Select Type CFS / CY</option>
                            <option value="1">CFS</option>
                            <option value="2">CY</option>
                        </select>
                    </div>
                </div>
            </div>



        </div>
    </div>

    <div class="clearfix"></div>
    <div class="row">
        <div class="col-md-12 col-sm-12 col-xs-12">
            <div class="x_panel">
                <div class="x_content" id="">
                    <div class="row">
                        <div class="col-sm-4">
                            <div class="form-group">
                                <label class="control-label  col-md-4 col-xs-12" for="first-name">
                                    Shipping Agent
                                </label>
                                <div class="col-md-8 col-xs-12">
                                    @Html.DropDownList("ShippingAgentId", agents, "Select Shipping Agent", new { @class = "form-control select2", @id = "agentId", required = "required" })

                                </div>
                            </div>
                        </div>
                        <div class="col-md-4 col-xs-12">
                            <div class="control-group">
                                <div class="controls">
                                    <label class="control-label col-md-4 col-xs-12" for="first-name">
                                        From:
                                    </label>
                                    <div class="col-md-8 col-xs-12">
                                        <input type="date" class="form-control" id="fromdate">
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-4 col-xs-12">
                            <div class="control-group">
                                <div class="controls">
                                    <label class="control-label col-md-4 col-xs-12" for="first-name">
                                        To:
                                    </label>
                                    <div class="col-md-8 col-xs-12">
                                        <input type="date" class="form-control" id="todate">
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <br />
                    <div class="row">
                        <div class="row">
                            <div class="col-md-12 col-xs-12">
                                <button type="button" class="btn btn-dark mobBtn" onclick="myFunction()" style="float:right">Submit</button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="row">

        <div class="col-md-12 col-sm-12 col-xs-12">
            <div class="x_panel">
                <div id="large-indicator"></div>
                <div class="x_content" id="repotdata">

                </div>
            </div>
        </div>

    </div>

</div>
<script>
    $('.select2').select2();
</script>