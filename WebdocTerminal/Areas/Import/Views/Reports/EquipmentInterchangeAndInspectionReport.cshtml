<link rel="stylesheet" href="~/css/viewer.part.bundle.css" />
<script src="~/js/viewer.part.bundle.js"></script>
<script src="~/js/app.js/EquipmentInterchangeAndInspectionReport.js"></script>



<div class="right_col" role="main">
    <div class="page-title">
        <div class="title_left">
            <h3>
                EQUIPMENT INTERCHANGE AND INSPECTION REPORT
            </h3>
        </div>
    </div>
    <div class="clearfix"></div>

    <div class="row">
        <div class="col-md-12 col-sm-12 col-xs-12">
            <div class="x_panel">
                <div class="x_content" id="">

                    <div class="row">
                        @{ Html.RenderPartial("_Filters_AllContainers"); }


                    </div>

                    <div class="row">
                        <div class="col-md-12 col-sm-12 col-xs-12">
                            <select class="form-control" id="status">
                                <option value="EMPTY DELIVERY">EMPTY DELIVERY</option>
                                <option value="IMPORT DELIVERY">IMPORT DELIVERY</option>
                            </select>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-12">
                            <button type="button" id="btnSubmitReport" class="btn btn-dark" style="float: right;" onclick="myFunction()">Submit</button>
                        </div>

                    </div>

                </div>
            </div>
        </div>
    </div>

    <div class="row">
        <div class="col-md-12 col-sm-12 col-xs-12">
            <div class="x_panel">
                <div id="large-indicator"></div>
                <div class="x_content" id="repotdata">
                </div>
            </div>
        </div>

    </div>

</div>
