
@{

    var Bank = (IEnumerable<SelectListItem>)ViewData["Bank"];

}
<link rel="stylesheet" href="~/css/viewer.part.bundle.css" />
<script src="~/js/viewer.part.bundle.js"></script>
<script src="~/js/app.js/PayOrderReport.js"></script>


<div class="right_col" role="main">
    <div class="page-title x_panel">
        <div class="title_left">
            <h3>
                Payorder Report
            </h3>
        </div>
    </div>

    <div class="clearfix"></div>
    <div class="row">
        <div class="col-md-12 col-sm-12 col-xs-12">
            <div class="x_panel">
                <div class="x_content" id="">

                    <div class="row">
                        <div class="col-md-5 col-xs-12">
                            <div class="form-group">
                                <label class="control-label col-md-4 col-sm-4 col-xs-12" for="first-name">
                                    Request No
                                </label>
                                <div class="col-md-8 col-sm-8 col-xs-12">
                                    @Html.DropDownList("BankId", Bank, "Select Bank", new { @class = "form-control select2", @id = "bank", @required = "required" })
                                </div>
                            </div>
                        </div>
                        <div class="col-md-5 col-xs-12">
                        </div>

                        <div class="col-lg-2 col-md-2 col-sm-2 col-xs-12 pad">
                            <button type="button" id="btnSubmitReport" style=" float: right;" class="btn btn-dark" onclick="myFunction()">Submit</button>
                        </div>

                    </div>





                </div>
            </div>
        </div>
    </div>


    <div class="clearfix"></div>

    <div class="row">

        <div class="col-md-12 col-sm-12 col-xs-12">
            <div class="x_panel">
                <div id="large-indicator"></div>
                <div class="x_content" id="repotdata">

                </div>
            </div>
        </div>

    </div>


</div>





<script>
    $('.select2').select2();
</script>
