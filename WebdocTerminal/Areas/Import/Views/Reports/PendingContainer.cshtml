@{
    var Agents = (IEnumerable<SelectListItem>)ViewData["Agents"];

}
<link rel="stylesheet" href="~/css/viewer.part.bundle.css" />
<script src="~/js/viewer.part.bundle.js"></script>
<script src="~/js/app.js/PendingContainer.js"></script>

<style>
    .newbox {
        text-align: center;
        padding: 3px 0px 3px 0px;
        font-size: 14px;
        font-weight: 600;
        margin: -3px -10px 10px -10px;
        color: white;
        background: #2a3f54;
        border-bottom: 1px solid #d1d1d1;
    }
</style>



<div class="right_col" role="main">
    <div class="page-title x_panel">
        <div class="title_left">
            <h3>
                Pending Containers
            </h3>
        </div>

    </div>
    <div class="clearfix"></div>

    <div class="row">
        <div class="col-md-12 col-sm-12 col-xs-12">
            <div class="x_panel">
                <div class="x_content" id="">


                    <div class="form-horizontal form-label-left row">


                        <div class="row">
                            <div class="col-lg-3">
                                <div class="form-group">
                                    <label class="control-label  col-md-3" for="first-name">
                                        Port
                                    </label>
                                    <div class="col-md-9">
                                        <select class="item form-control" name="ContainerType" id="port">
                                            <option value="">Please Select</option>
                                            <option value="%KPPI%">QICT</option>
                                            <option value="%KAPW%">KICT</option>
                                            <option value="%KAPE%">PICT</option>
                                            <option value="%KAPS%">SAPT</option>
                                        </select>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="form-group">
                                    <label class="control-label  col-md-3" for="first-name">
                                        Line
                                    </label>
                                    <div class="col-md-9">
                                        @Html.DropDownList("ShippingAgentId", Agents, "Select Line", new { @class = "form-control select2", @id = "shippingAgent", required = "required" })
                                    </div>
                                </div>
                            </div>
 
                        </div>

                        <div class="row">






                        </div>



                        <div class="row">

                            <div class="col-lg-10 col-md-10 col-sm-2 col-xs-12 pad">

                            </div>

                            <div class="col-lg-2 col-md-2 col-sm-2 col-xs-12 pad">
                                <button type="button" id="btnSubmitReport" class="btn btn-dark" onclick="myFunction()" style="float: right;">Print</button>
                            </div>


                        </div>


                    </div>
                </div>
            </div>
        </div>




        <div class="clearfix"></div>

        <div class="row">

            <div class="col-md-12 col-sm-12 col-xs-12">
                <div class="x_panel">
                    <div id="large-indicator"></div>
                    <div class="x_content" id="repotdata">

                    </div>
                </div>
            </div>

        </div>

    </div>



    <script>
        $('.select2').select2();
    </script>
