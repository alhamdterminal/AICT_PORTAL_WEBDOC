@{
    var patry = (IEnumerable<SelectListItem>)ViewData["patry"];
}
<link rel="stylesheet" href="~/css/viewer.part.bundle.css" />
<script src="~/js/viewer.part.bundle.js"></script>
<script src="~/js/app.js/PartyLedgerPatryWise.js"></script>

<div class="right_col" role="main">
    <div class="page-title x_panel">
        <div class="title_left">
            <h3>
                Party Ledger
            </h3>
        </div>
        <div class="title_right">


        </div>
    </div>
    <div class="clearfix"></div>




    <div class="row">
        <div class="col-md-12 col-sm-12 col-xs-12">
            <div class="x_panel">
                <div class="x_content" id="">
                    <div class="row">
                        <div class="col-md-4">
                            <div class="form-group">
                                <label class="control-label  col-md-4 col-xs-12" for="first-name">
                                    Party <span class="required">*</span>
                                </label>
                                <div class="col-md-8 col-xs-12">
                                    @Html.DropDownList("PatryId", patry, "Select Party", new { @class = "form-control select2", @id = "partyId", required = "required", @onchange = "callChangefunc()" })

                                </div>
                            </div>
                        </div>

                    </div>
                </div>
            </div>
        </div>
    </div>







    <div class="row">

        <div class="col-md-12 col-sm-12 col-xs-12">
            <div class="x_panel">
                <div id="large-indicator"></div>
                <div class="x_content" id="repotdata">

                </div>
            </div>
        </div>

    </div>

</div>

<script>
    $('.select2').select2();
</script>