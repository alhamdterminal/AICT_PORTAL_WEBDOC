<link rel="stylesheet" href="~/css/viewer.part.bundle.css" />
<script src="~/js/viewer.part.bundle.js"></script>
<script src="~/js/app.js/ShedBrtContainerWise.js"></script>



<div class="right_col" role="main">
    <div class="page-title">
        <div class="title_left">
            <h3>
                Shed Brt Container Wise
            </h3>

        </div>


    </div>
    <div class="clearfix"></div>
    <div class="row">
        <div class="col-md-12 col-sm-12 col-xs-12">
            <div class="x_panel">
                <div class="x_content" id="">



                    <div class="col-md-12 col-sm-12 col-xs-12">
                        <div class="x_panel">
                            <div class="x_content" id="">


                                <div class="row">
                                    <div class="col-md-4 col-sm-4 col-xs-12">
                                        <div class="item form-group">
                                            <label class="control-label col-md-4 col-sm-3 col-xs-12" for="name">
                                                Container No: <span class="required">*</span>
                                            </label>
                                            <div class="col-md-8 col-sm-8 col-xs-12">
                                                @(Html.DevExtreme()
                                                    .Autocomplete()
                                                    .ID("IGM")
                                                    .DataSource(d => d.Mvc()
                                                    .Controller("APICalls")
                                                    .Area("")
                                                    .LoadAction("GetIGMOContainerNumbers"))
                                                    .Placeholder("Type Container No...")
                                                    .OnValueChanged("igm_changed"))
                                            </div>
                                        </div>
                                    </div>


                                </div>

                                <div class="row">
                                    <div class="col-md-12">
                                        <button type="button" id="btnSubmitReport" class="btn btn-dark" onclick="printreport()" style="float:right">Submit</button>
                                    </div>
                                </div>




                            </div>
                        </div>
                    </div>


                </div>
            </div>
        </div>

    </div>


    <div class="row">

        <div class="col-md-12 col-sm-12 col-xs-12">
            <div class="x_panel">
                <div id="large-indicator"></div>
                <div class="x_content" id="repotdata">


                </div>
            </div>
        </div>

    </div>

</div>



