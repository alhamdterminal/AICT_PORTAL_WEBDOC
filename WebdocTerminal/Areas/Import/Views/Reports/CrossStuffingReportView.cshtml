<link rel="stylesheet" href="~/css/viewer.part.bundle.css" />
<script src="~/js/viewer.part.bundle.js"></script>
<script src="~/js/app.js/CrossStuffingReportView.js"></script>


<div class="right_col" role="main">
    <div class="page-title x_panel">
        <div class="title_left">
            <h3>
                Cross Stuffing
            </h3>
        </div>
    </div>
    <div class="clearfix"></div>
    <div class="row">
        <div class="col-md-12 col-sm-12 col-xs-12">
            <div class="x_panel">
                <div class="x_content" id="">

                    <div class="row">

                        <div class="col-md-3">
                            <div class="item form-group">
                                <label class="control-label col-md-3 col-sm-3 col-xs-12" for="name">
                                    BL No: <span class="required">*</span>
                                </label>
                                <div class="col-md-9">
                                    @(Html.DevExtreme()
                                                .Autocomplete()
                                                .ID("bllist")
                                                .DataSource(d => d.Mvc()
                                                .Controller("APICalls")
                                                .Area("")
                                                .LoadAction("GetBLNumberDetail"))
                                                .Placeholder("Type BL No..."))
                                </div>
                            </div>
                        </div>
                        <div class="col-md-9 col-sm-9 col-xs-12">
                            <div class="form-group">
                                <div>
                                    <i class="bi bi-arrow-repeat"></i>
                                    <button id="btnSubmit" type="button" onclick="myFunction()" class="btn btn-danger" style="float:right;">Print</button>

                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="row">

        <div class="col-md-12 col-sm-12 col-xs-12">
            <div class="x_panel">
                <div id="large-indicator"></div>
                <div class="x_content" id="repotdata">

                </div>
            </div>
        </div>

    </div>

</div>


<script>
    $('.select2').select2();
</script>
