@{
    var agents = (IEnumerable<SelectListItem>)ViewData["Agents"];
}


<style>
    @@media only screen and (min-device-width : 320px) and (max-device-width : 480px) {


        .mobBtn {
            margin-top: 23px;
        }
    }
</style>
<link rel="stylesheet" href="~/css/viewer.part.bundle.css" />
<script src="~/js/viewer.part.bundle.js"></script>
<script src="~/js/app.js/ContainerAvailableToYard.js"></script>

<div class="right_col" role="main">
    <div class="x_panel">
        <div class="page-title ">
            <div class="title_left">
                <h3>
                    Empty Container Available At Yard
                </h3>
            </div>
            <div class="title_right">



            </div>
        </div>
    </div>

    <div class="clearfix"></div>
    <div class="row">
        <div class="col-md-12 col-sm-12 col-xs-12">
            <div class="x_panel">
                <div class="x_content" id="">

                    <div class="form-group">
                        <label class="control-label  col-md-2 col-xs-12" for="first-name">
                            Shipping Agent
                        </label>
                        <div class="col-md-4 col-xs-12">
                            @Html.DropDownList("ShippingAgentId", agents, "Select Shipping Agent", new { @class = "form-control select2", @id = "agentId", required = "required" })

                        </div>

                        <div class="col-md-6 col-xs-12">
                            <button class="btn btn-primary mobBtn" style="float:right" onclick="myFunction()">Submit</button>
                        </div>
                    </div>

                </div>
            </div>
        </div>
    </div>
    <div class="row">

        <div class="col-md-12 col-sm-12 col-xs-12">
            <div class="x_panel">
                <div id="large-indicator"></div>
                <div class="x_content" id="repotdata">

                </div>
            </div>
        </div>

    </div>
</div>


<script>
    $('.select2').select2();
</script>