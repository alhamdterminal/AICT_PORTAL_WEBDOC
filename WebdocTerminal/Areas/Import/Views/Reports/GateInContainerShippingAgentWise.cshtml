@{
    var agents = (IEnumerable<SelectListItem>)ViewData["Agents"];
}
<link rel="stylesheet" href="~/css/viewer.part.bundle.css" />
<script src="~/js/viewer.part.bundle.js"></script>
<script src="~/js/app.js/GateInContainerShippingAgentWise.js"></script>


<div class="right_col" role="main">

    <div class="x_panel">
        <div class="page-title ">
            <div class="">
                <div class="row">
                    <div class="col-lg-5 col-xs-8" style="float:left;">
                        <h3>
                            Gate In Container
                        </h3>
                    </div>
                    <div class="col-lg-5 col-xs-4" style="float:right;">
                        <div class="control-group">
                            <div class="controls">
                                <label class="control-label col-md-3 col-sm-3 col-xs-12">
                                    Type:
                                </label>
                                <div class="col-md-9">
                                    <select class="form form-control" id="type">
                                        <option class="form form-control" value="CFS / CY">CFS / CY</option>
                                        <option class="form form-control" value="CFS">CFS</option>
                                        <option class="form form-control" value="CY">CY</option>
                                    </select>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

            </div>
            <div class="title_right">

            </div>



        </div>
    </div>


    <div class="clearfix"></div>


    <div class="row">
        <div class="col-md-12 col-sm-12 col-xs-12">
            <div class="x_panel">
                <div class="x_content" id="">


                    <div class="row">

                        <div class="col-md-4">
                            <div class="form-group">
                                <label class="control-label  col-md-4 col-xs-12" for="first-name">
                                    Shipping Agent
                                </label>
                                <div class="col-md-8 col-xs-12">
                                    @Html.DropDownList("ShippingAgentId", agents, "Select Shipping Agent", new { @class = "form-control select2", @id = "agentId", required = "required" })

                                </div>
                            </div>
                        </div>

                        <div class="col-md-8">

                            @{Html.RenderPartial("_DateFilter"); }
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="row">

        <div class="col-md-12 col-sm-12 col-xs-12">
            <div class="x_panel">
                <div id="large-indicator"></div>
                <div class="x_content" id="repotdata">

                </div>
            </div>
        </div>

    </div>

</div>



<script>
    $('.select2').select2();
</script>