@{

    var shippingagent = (IEnumerable<SelectListItem>)ViewData["shippingagent"];

}

<link rel="stylesheet" href="~/css/viewer.part.bundle.css" />
<script src="~/js/viewer.part.bundle.js"></script>
<script src="~/js/app.js/BreakupofCashierDailySaleReportView.js"></script>


<div class="right_col" role="main">
    <div class="page-title x_panel">
        <div class="title_left">
            <h3>
                Breakup of Cashier Daily Sale Report
            </h3>
        </div>
        <div class="title_right">

        </div>
    </div>

    <div class="clearfix"></div>
    <div class="row">


    </div>

    <div class="row">
        <div class="col-md-12 col-sm-12 col-xs-12">
            <div class="x_panel">
                <div class="x_content" id="">



                    <div class="col-md-12 col-sm-12 col-xs-12">
                        <div class="x_panel">
                            <div class="x_content" id="">


                                <div class="row">
                                    <div class="col-md-4 col-sm-4 col-xs-12">
                                        <div class="form-group">
                                            <label class="control-label  col-md-4 col-xs-12" for="first-name">
                                                FF <span class="required">*</span>
                                            </label>
                                            <div class="col-md-8 col-xs-12">
                                                @Html.DropDownList("ShippingAgentId", shippingagent, "Select FF", new { @class = "form-control select2", @disabled = true, @id = "shippingagentId" })
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-8 col-sm-8 col-xs-12"></div>
                                    </div>


                                    <div class="row">

                                        <div class="col-md-4">
                                            <div class="form-group">
                                                <label class="control-label  col-md-3" for="first-name">
                                                    Type
                                                </label>
                                                <div class="col-md-9">
                                                    <select class="item form-control" name="ContainerType" id="type">
                                                        <option value="">Please Select</option>
                                                        <option value="Cash">Cash</option>
                                                        <option value="PO">PO</option>
                                                        <option value="Cheque">Cheque</option>
                                                        <option value="Online">Online</option>
                                                        <option value="BTL">BTL</option>
                                                        <option value="CashRefund">CashRefund</option>
                                                        <option value="CreditAllowed">CreditAllowed</option>
                                                        <option value="ExcessAmount">ExcessAmount</option>
                                                        <option value="KnockOff">KnockOff</option>
                                                        <option value="LetPass">LetPass</option>
                                                    </select>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-8">
                                            @{Html.RenderPartial("_DateFilter"); }
                                        </div>
                                    </div>


                                </div>
                        </div>
                    </div>


                </div>
            </div>
        </div>

    </div>


    <div class="row">

        <div class="col-md-12 col-sm-12 col-xs-12">
            <div class="x_panel">
                <div id="large-indicator"></div>
                <div class="x_content" id="repotdata">

                </div>
            </div>
        </div>

    </div>

</div>



<script>
    $('.select2').select2();
</script>


