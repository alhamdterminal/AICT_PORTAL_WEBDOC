
@{
    var agents = (IEnumerable<SelectListItem>)ViewData["Agents"];
}
<link rel="stylesheet" href="~/css/viewer.part.bundle.css" />
<script src="~/js/viewer.part.bundle.js"></script>
<script src="~/js/app.js/DeStuffingContainerWise.js"></script>

<style>

    .mobBtn {
        margin-top: 23px;
    }
</style>



<div class="right_col" role="main">
    <div class="page-title">
        <div class="title_left">
            <h3> Destuffing Container Wise </h3>
        </div>

    </div>
    <div class="clearfix"></div>

    <div class="row">
        <div class="col-md-12">
            <div class="x_panel">
                <div class="x_content">


                    <div class="row" style="margin-top: 13px;">
                        <div class="col-sm-4">
                            <div class="form-group">
                                <label class="control-label  col-md-4 col-xs-12" for="first-name">
                                    Shipping Agent
                                </label>
                                <div class="col-md-8 col-xs-12">
                                    @Html.DropDownList("ShippingAgentId", agents, "Select Shipping Agent", new { @class = "form-control", @id = "agentId", required = "required" })

                                </div>
                            </div>
                        </div>



                        <div class="col-md-4 col-xs-12">
                            <div class="control-group">
                                <div class="controls">
                                    <label class="control-label col-md-4 col-xs-12" for="first-name">
                                        From:
                                    </label>
                                    <div class="col-md-8 col-xs-12">
                                        <input type="date" class="form-control" id="fromdate">
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-4 col-xs-12">
                            <div class="control-group">
                                <div class="controls">
                                    <label class="control-label col-md-4 col-xs-12" for="first-name">
                                        To:
                                    </label>
                                    <div class="col-md-8 col-xs-12">
                                        <input type="date" class="form-control" id="todate">
                                    </div>
                                </div>
                            </div>
                        </div>

                    </div>
                    <div class="row">
                        <div class="col-md-12 col-xs-12">
                            <button type="button" class="btn btn-dark mobBtn" onclick="myFunction()" style="float:right">Submit</button>
                        </div>
                    </div>
                    <div class="clearfix"></div>

                    <div class="row">

                        <div class="col-md-12 col-sm-12 col-xs-12">
                            <div class="x_panel">
                                <div id="large-indicator"></div>
                                <div class="x_content" id="repotdata">

                                </div>
                            </div>
                        </div>

                    </div>
                </div>


                <div class="ln_solid"></div>

            </div>
        </div>
    </div>
</div>
