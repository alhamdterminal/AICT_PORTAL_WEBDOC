@{
    var shippingAgents = (IEnumerable<SelectListItem>)ViewData["shippingAgents"];
}
<link rel="stylesheet" href="~/css/viewer.part.bundle.css" />
<script src="~/js/viewer.part.bundle.js"></script>
<script src="~/js/app.js/CFSReleasedContainers.js"></script>

<div class="right_col" role="main">
    <div class="page-title x_panel">
        <div class="title_left">
            <h3>
                Released Indexes
            </h3>
        </div>
        <div class="title_right">

        </div>
    </div>
    <div class="clearfix"></div>

    <div class="row">

    </div>
    <div class="row">
        <div class="col-md-12 col-sm-12 col-xs-12">
            <div class="x_panel">
                <div class="x_content" id="">
                    <div class="form-horizontal form-label-left row">
                        <div class="col-md-4 col-sm-4 col-xs-12">
                            <div class="item form-group">
                                <label class="control-label col-md-4 col-sm-4 col-xs-12" for="name">
                                    Shipping Agent <span class="required">*</span>
                                </label>
                                <div class="col-md-8 col-sm-8 col-xs-12">
                                    @Html.DropDownList("ShippingAgentId", shippingAgents, "Select Shipping Agent", new { @class = "form-control select2", @id = "shippingAgent", required = "required" })

                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-4">
                            <div class="form-group">
                                <label class="control-label  col-md-3" for="first-name">
                                    From :
                                </label>
                                <div class="col-md-9">
                                    <input type="date" id="fromdate" class="form-control" />
                                </div>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="form-group">
                                <label class="control-label  col-md-3" for="first-name">
                                    To :
                                </label>
                                <div class="col-md-9">
                                    <input type="date" id="todate" class="form-control" />
                                </div>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <button type="button" id="btnSubmitReport" class="btn btn-dark" onclick="myFunction()" style="float:right">Submit</button>
                        </div>
                    </div>



                </div>
            </div>
        </div>
    </div>
    <div class="row">

        <div class="row">

            <div class="col-md-12 col-sm-12 col-xs-12">
                <div class="x_panel">
                    <div id="large-indicator"></div>
                    <div class="x_content" id="repotdata">

                    </div>
                </div>
            </div>

        </div>

    </div>

</div>

