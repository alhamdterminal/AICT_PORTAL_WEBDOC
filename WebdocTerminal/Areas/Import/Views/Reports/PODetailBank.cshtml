

@{
    var Banks = (IEnumerable<SelectListItem>)ViewData["Banks"];
    var shippingagent = (IEnumerable<SelectListItem>)ViewData["shippingagent"];

}
<link rel="stylesheet" href="~/css/viewer.part.bundle.css" />
<script src="~/js/viewer.part.bundle.js"></script>
<script src="~/js/app.js/PODetailBank.js"></script>

<style>
    .newbox {
        text-align: center;
        padding: 3px 0px 3px 0px;
        font-size: 14px;
        font-weight: 600;
        margin: -3px -10px 10px -10px;
        color: white;
        background: #2a3f54;
        border-bottom: 1px solid #d1d1d1;
    }
</style>



<div class="right_col" role="main">
    <div class="page-title x_panel">
        <div class="title_left">
            <h3>
                Daily Sale For Bank

            </h3>
        </div>

    </div>
    <div class="clearfix"></div>

    <div class="row">
        <div class="col-md-12 col-sm-12 col-xs-12">
            <div class="x_panel">
                <div class="x_content" id="">


                    <div class="form-horizontal form-label-left row">

  
                        <div class="row">



                            <div class="col-md-3">
                                <div class="control-group">
                                    <div class="controls">
                                        <label class="control-label col-md-3 col-sm-3 col-xs-12" for="first-name">
                                            From:
                                        </label>
                                        <div class="col-md-9 col-xs-12">
                                            <input type="text" class="form-control has-feedback-left" id="single_cal2" name="fromdate" aria-describedby="inputSuccess2Status2">
                                            <span class="fa fa-calendar-o form-control-feedback left" aria-hidden="true"></span>
                                            <span id="inputSuccess2Status2" class="sr-only">(success)</span>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="col-md-3">
                                <div class="control-group">
                                    <div class="controls">
                                        <label class="control-label col-md-3 col-sm-2 col-xs-12" for="first-name">
                                            To:
                                        </label>
                                        <div class="col-md-9 col-xs-12">
                                            <input type="text" class="form-control has-feedback-left" id="single_cal3" name="todate" aria-describedby="inputSuccess2Status2">
                                            <span class="fa fa-calendar-o form-control-feedback left" aria-hidden="true"></span>
                                            <span id="inputSuccess2Status2" class="sr-only">(success)</span>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="col-md-3">
                                <div class="control-group">
                                    <div class="controls">
                                        <label class="control-label col-md-3 col-sm-3 col-xs-12" for="first-name">
                                            From Range:
                                        </label>
                                        <div class="col-md-9 col-xs-12">
                                            <input type="number" id="fromrange" class="form-control" />
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="col-md-3">
                                <div class="control-group">
                                    <div class="controls">
                                        <label class="control-label col-md-3 col-sm-2 col-xs-12" for="first-name">
                                            To Range:
                                        </label>
                                        <div class="col-md-9 col-xs-12">
                                            <input type="number" id="torange" class="form-control" />
                                        </div>
                                    </div>
                                </div>
                            </div>

                        </div>

                        <div class="row">



                            <div class="col-md-3">
                                <div class="control-group">
                                    <div class="controls">
                                        <label class="control-label col-md-3 col-sm-2 col-xs-12" for="first-name">
                                            Date:
                                        </label>
                                        <div class="col-md-9 col-xs-12">
                                            <input type="text" class="form-control has-feedback-left" id="single_cal4" name="todate" aria-describedby="inputSuccess2Status2">
                                            <span class="fa fa-calendar-o form-control-feedback left" aria-hidden="true"></span>
                                            <span id="inputSuccess2Status2" class="sr-only">(success)</span>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="col-md-3">
                                <div class="control-group">
                                    <div class="controls">
                                        <label class="control-label col-md-3 col-sm-2 col-xs-12" for="first-name">
                                            Branch:
                                        </label>
                                        <div class="col-md-9 col-xs-12">
                                            <input type="text" id="branch" class="form-control" />
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="col-md-3">
                                <div class="control-group">
                                    <div class="controls">
                                        <label class="control-label col-md-3">
                                            Banks:
                                        </label>
                                        <div class="col-md-9">
                                            @Html.DropDownList("BankId", Banks, "Select Bank", new { @class = "form-control select2", @id = "bankid" })
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="control-group">
                                    <div class="controls">
                                        <label class="control-label col-md-3 col-sm-2 col-xs-12" for="first-name">
                                            A\C Title:
                                        </label>
                                        <div class="col-md-9 col-xs-12">
                                            <input type="text" id="title" class="form-control" value="AL Hamd International" />
                                        </div>
                                    </div>
                                </div>
                            </div>

                        </div> 
                        <div class="row">

                            <div class="col-lg-3 col-md-3 col-sm-3">
                                <div class="form-group">
                                    <label class="control-label  col-md-3" for="first-name">
                                        Amount Range
                                    </label>
                                    <div class="col-md-9">
                                        <select class="item form-control" id="amountRange">
                                            <option value="">Please Select</option>
                                            <option value="<=100K"><= 100K</option>
                                            <option value=">100k">Greater Then 100K</option>
                                        </select>
                                    </div>
                                </div>
                            </div>
                            <div class="col-lg-3 col-md-3 col-sm-3">
                                <div class="form-group">
                                    <label class="control-label  col-md-4 col-xs-12" for="first-name">
                                        FF <span class="required">*</span>
                                    </label>
                                    <div class="col-md-8 col-xs-12">
                                        @Html.DropDownList("ShippingAgentId", shippingagent, "Select FF", new { @class = "form-control select2", @disabled = true, @id = "shippingagentId" })
                                    </div>
                                </div>
                            </div>
                            <div class="col-lg-9 col-md-9 col-sm-9 col-xs-12 pad">
                                <button type="button" id="btnSubmitReport" class="btn btn-dark" onclick="myFunction()" style="float: right;">Print</button>
                            </div>


                        </div>


                    </div>
                </div>
            </div>
        </div>




        <div class="clearfix"></div>

        <div class="row">

            <div class="col-md-12 col-sm-12 col-xs-12">
                <div class="x_panel">
                    <div id="large-indicator"></div>
                    <div class="x_content" id="repotdata">

                    </div>
                </div>
            </div>

        </div>

    </div>
 
 </div>


    <script>
    $('.select2').select2();
    </script>
