@{
    var GoodsHeadItems = (IEnumerable<SelectListItem>)ViewData["GoodsHeadItems"];
}

<link rel="stylesheet" href="~/css/viewer.part.bundle.css" />
<script src="~/js/viewer.part.bundle.js"></script>
<script src="~/js/app.js/shedexamination.js"></script>


<div class="right_col" role="main">
    <div class="page-title x_panel">
        <div class="title_left">
            <h3>
                CFS Examination
            </h3>
        </div>
        <div class="title_right">
        </div>
    </div>

    <div class="clearfix"></div>
    <div class="row">


    </div>

    <div class="row">
        <div class="col-md-12 col-sm-12 col-xs-12">
            <div class="x_panel">
                <div class="x_content" id="">



                    <div class="col-md-12 col-sm-12 col-xs-12">
                        <div class="x_panel">
                            <div class="x_content" id="">


                                <div class="row">
                                    <div class="col-md-4">
                                        <div class="form-group">
                                            <label class="control-label  col-md-3" for="commodity">
                                                Commodity:
                                            </label>
                                            <div class="col-md-9">
                                                @Html.DropDownList("GoodsHeadName", GoodsHeadItems, "Select Commodity", new { @class = "form-control select2", @id = "commodity", required = "required" })
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-4">
                                        <div class="form-group">
                                            <label class="control-label  col-md-2" for="fromdate">
                                                From :
                                            </label>
                                            <div class="col-md-10">
                                                <input type="date" id="fromdate" class="form-control" />
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-4">
                                        <div class="form-group">
                                            <label class="control-label  col-md-2" for="todate">
                                                To :
                                            </label>
                                            <div class="col-md-10">
                                                <input type="date" id="todate" class="form-control" />
                                            </div>
                                        </div>
                                    </div>
                                </div><br />
                                <div class="row">
                                    <div class="col-md-12">
                                        <button type="button" id="btnSubmitReport" class="btn btn-dark" onclick="myFunction()" style="float:right">Submit</button>
                                    </div>
                                </div>




                            </div>
                        </div>
                    </div>


                </div>
            </div>
        </div>

    </div>


    <div class="row">

        <div class="col-md-12 col-sm-12 col-xs-12">
            <div class="x_panel">
                <div id="large-indicator"></div>
                <div class="x_content" id="repotdata">

                </div>
            </div>
        </div>

    </div>

</div>


