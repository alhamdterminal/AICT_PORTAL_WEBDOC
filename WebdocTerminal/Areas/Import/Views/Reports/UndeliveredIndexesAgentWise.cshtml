@{
    var Agents = (IEnumerable<SelectListItem>)ViewData["Agents"];
    var ClearingAgent = (IEnumerable<SelectListItem>)ViewData["ClearingAgent"];
    var Good = (IEnumerable<SelectListItem>)ViewData["Good"];

}
<link rel="stylesheet" href="~/css/viewer.part.bundle.css" />
<script src="~/js/viewer.part.bundle.js"></script>
<script src="~/js/app.js/UndeliveredIndexesAgentWise.js"></script>

<style>
    .newbox {
        text-align: center;
        padding: 3px 0px 3px 0px;
        font-size: 14px;
        font-weight: 600;
        margin: -3px -10px 10px -10px;
        color: white;
        background: #2a3f54;
        border-bottom: 1px solid #d1d1d1;
    }
</style>



<div class="right_col" role="main">
    <div class="page-title x_panel">
        <div class="title_left">
            <h3>
                Un Delivered Indexes
            </h3>
        </div>

    </div>
    <div class="clearfix"></div>

    <div class="row">
        <div class="col-md-12 col-sm-12 col-xs-12">
            <div class="x_panel">
                <div class="x_content" id="">


                    <div class="form-horizontal form-label-left row">


                        <div class="row">
                            <div class="col-lg-3">
                                <div class="form-group">
                                    <label class="control-label  col-md-3" for="first-name">
                                        Cargo Type
                                    </label>
                                    <div class="col-md-9">
                                        <select class="item form-control" name="ContainerType" id="cargoType">
                                            <option value="">Please Select</option>
                                            <option value="FCL">FCL</option>
                                            <option value="LCL">LCL</option>
                                        </select>
                                    </div>
                                </div>
                            </div>
                            <div class="col-lg-3">
                                <div class="form-group">
                                    <label class="control-label  col-md-3" for="first-name">
                                        Type
                                    </label>
                                    <div class="col-md-9">
                                        <select class="item form-control" name="ContainerType" id="containerType">
                                            <option value="0">Please Select</option>
                                            <option value="1">CFS</option>
                                            <option value="2">CY</option>
                                        </select>
                                    </div>
                                </div>
                            </div>

                            <div class="col-lg-3">
                                <div class="control-group">
                                    <div class="controls">
                                        <label class="control-label col-md-3 col-sm-3 col-xs-12" for="first-name">
                                            From:
                                        </label>
                                        <div class="col-md-9 col-xs-12">
                                            <input type="text" class="form-control has-feedback-left" id="single_cal2" name="fromdate" aria-describedby="inputSuccess2Status2">
                                            <span class="fa fa-calendar-o form-control-feedback left" aria-hidden="true"></span>
                                            <span id="inputSuccess2Status2" class="sr-only">(success)</span>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="col-lg-3">
                                <div class="control-group">
                                    <div class="controls">
                                        <label class="control-label col-md-2 col-sm-2 col-xs-12" for="first-name">
                                            To:
                                        </label>
                                        <div class="col-md-9 col-xs-12">
                                            <input type="text" class="form-control has-feedback-left" id="single_cal3" name="todate" aria-describedby="inputSuccess2Status2">
                                            <span class="fa fa-calendar-o form-control-feedback left" aria-hidden="true"></span>
                                            <span id="inputSuccess2Status2" class="sr-only">(success)</span>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="row">

                            <div class="col-lg-3">
                                <div class="control-group">
                                    <div class="controls">
                                        <label class="control-label col-md-3" for="first-name">
                                            Consignee
                                        </label>
                                        <div class="col-md-9" id="searchBoxForConsigne"  >

                                        </div>
                                    </div>
                                </div>
                            </div>


                            <div class="col-md-3">
                                <div class="form-group">
                                    <label class="control-label  col-md-3" for="first-name">
                                        ShippingAgent
                                    </label>
                                    <div class="col-md-9">
                                        @Html.DropDownList("ShippingAgentId", Agents, "Select Shipping Agent", new { @class = "form-control select2", @id = "shippingAgent", required = "required" })
                                    </div>
                                </div>
                            </div>


                            <div class="col-md-3">
                                <div class="control-group">
                                    <div class="controls">
                                        <label class="control-label col-md-3">
                                            ClearingAgent:
                                        </label>
                                        <div class="col-md-9">
                                            @Html.DropDownList("ClearingAgentId", ClearingAgent, "Select ClearingAgent", new { @class = "form-control select2", @id = "clearingAgent", required = "required" })
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="control-group">
                                    <div class="controls">
                                        <label class="control-label col-md-3">
                                            Goods Head:
                                        </label>
                                        <div class="col-md-9">
                                            @Html.DropDownList("GoodsHeadId", Good, "Select Good", new { @class = "form-control select2", @id = "goodheadid", required = "required" })
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>



                        <div class="row">

                            <div class="col-lg-10 col-md-10 col-sm-2 col-xs-12 pad">
                                 
                            </div>

                            <div class="col-lg-2 col-md-2 col-sm-2 col-xs-12 pad">
                                <button type="button" id="btnSubmitReport" class="btn btn-dark" onclick="myFunction()" style="float: right;">Print</button>
                            </div>


                        </div>
 

                    </div>
            </div>
        </div>
    </div>




    <div class="clearfix"></div>

    <div class="row">

        <div class="col-md-12 col-sm-12 col-xs-12">
            <div class="x_panel">
                <div id="large-indicator"></div>
                <div class="x_content" id="repotdata">

                </div>
            </div>
        </div>

    </div>

</div>



<script>
    $('.select2').select2();
</script>