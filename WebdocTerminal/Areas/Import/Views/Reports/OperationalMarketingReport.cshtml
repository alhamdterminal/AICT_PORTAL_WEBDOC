                                <div class="control-group">
                                    <div class="controls">
                                        <label class="control-label col-md-3">
                                            Goods Head:
                                        </label>
                                        <div class="col-md-9">
                                            @Html.DropDownList("GoodsHeadId", Good, "Select Good", new { @class = "form-control select2", @id = "goodheadid", required = "required" })
                                        </div>
                                    </div>
                                </div>
                            </div>*@

                            @*<div class="col-md-4">
                                <div class="control-group">
                                    <div class="controls">
                                        <label class="control-label col-md-3 col-sm-3 col-xs-12" for="first-name">
                                            From Delivered:
                                        </label>
                                        <div class="col-md-9 col-xs-12">
                                            <input type="date" id="fromdatedelivered" class="form-control" />
                                        </div>
                                    </div>
                                </div>
                            </div>*@

                            @*<div class="col-md-4">
                                <div class="control-group">
                                    <div class="controls">
                                        <label class="control-label col-md-2 col-sm-2 col-xs-12" for="first-name">
                                            To Delivered:
                                        </label>
                                        <div class="col-md-9 col-xs-12">
                                            <input type="date" id="todatedelivered" class="form-control" />
                                        </div>
                                    </div>
                                </div>
                            </div>*@

                        <!--</div>-->



                        <div class="row">

                            <div class="col-lg-10 col-md-10 col-sm-2 col-xs-12 pad">

                            </div>

                            <div class="col-lg-2 col-md-2 col-sm-2 col-xs-12 pad">
                                <button type="button" id="btnSubmitReport" class="btn btn-dark" onclick="myFunction()" style="float: right;">Print</button>
                            </div>


                        </div>


                    </div>
                </div>
            </div>
        </div>




        <div class="clearfix"></div>

        <div class="row">

            <div class="col-md-12 col-sm-12 col-xs-12">
                <div class="x_panel">
                    <div id="large-indicator"></div>
                    <div class="x_content" id="repotdata">

                    </div>
                </div>
            </div>

        </div>

    </div>

</div>



