

@{
    var CAgent = (IEnumerable<SelectListItem>)ViewData["CAgent"];

}
<link rel="stylesheet" href="~/css/viewer.part.bundle.css" />
<script src="~/js/viewer.part.bundle.js"></script>
<script src="~/js/app.js/OperationalMarketingReport.js"></script>
<style>
    .newbox {
        text-align: center;
        padding: 3px 0px 3px 0px;
        font-size: 14px;
        font-weight: 600;
        margin: -3px -10px 10px -10px;
        color: white;
        background: #2a3f54;
        border-bottom: 1px solid #d1d1d1;
    }
</style>


<div class="right_col" role="main">

    <div class="page-title x_panel">
        <div class="title_left">
            <h3>
                Operational Marketing Report

            </h3>
        </div>

    </div>

    <div class="clearfix"></div>

    <div class="row">

        <div class="col-md-12 col-sm-12 col-xs-12">
            <div class="x_panel">
                <div class="x_content" id="">


                    <div class="form-horizontal form-label-left row">


                        <div class="row">


                            <div class="col-lg-2">
                                <div class="form-group">
                                    <label class="control-label  col-md-3" for="first-name">
                                        Type:
                                    </label>
                                    <div class="col-md-9">
                                        <select class="item form-control" name="type" id="type">
                                            <option value="">All</option>
                                            <option value="CY">CY</option>
                                            <option value="CFS">CFS</option>
                                        </select>
                                    </div>
                                </div>
                            </div>

                            <div class="col-lg-4">
                                <div class="control-group">
                                    <div class="controls">
                                        <label class="control-label col-md-3 col-sm-3 col-xs-12" for="first-name">
                                            From:
                                        </label>
                                        <div class="col-md-9 col-xs-12">
                                            <input type="date" id="fromdate" class="form-control" />
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="col-lg-4">
                                <div class="control-group">
                                    <div class="controls">
                                        <label class="control-label col-md-2 col-sm-2 col-xs-12" for="first-name">
                                            To:
                                        </label>
                                        <div class="col-md-9 col-xs-12">
                                            <input type="date" id="todate" class="form-control" />
                                        </div>
                                    </div>
                                </div>
                            </div>


                        </div>

                        <br />

                        <div class="row">

                            <div class="col-lg-2">
                                <div class="form-group">
                                    <label class="control-label  col-md-3" for="first-name">
                                        Status:
                                    </label>
                                    <div class="col-md-9">
                                        <select class="item form-control" name="status" id="status">
                                            <option value="">All</option>
                                            <option value="Delivered">Delivered</option>
                                            <option value="Undelivered">Undelivered</option>
                                        </select>
                                    </div>
                                </div>
                            </div>


                            <div class="col-lg-4">
                                <div class="form-group">
                                    <label class="control-label  col-md-3" for="first-name">
                                        Clearing Agent:
                                    </label>
                                    <div class="col-md-9">
                                        @Html.DropDownList("ClearingAgentId", CAgent, "Select Clearing Agent", new { @class = "form-control select2", @id = "clearingagent", required = "required" })
                                    </div>
                                </div>
                            </div>

                            <div class="col-lg-4">
                                <div class="control-group">
                                    <div class="controls">
                                        <label class="control-label col-md-3" for="first-name">
                                            Consignee:
                                        </label>
                                        <div class="col-md-9" id="searchBoxForConsigne">

                                        </div>
                                    </div>
                                </div>
                            </div>


                        </div>

                        <!--<div class="row">-->

