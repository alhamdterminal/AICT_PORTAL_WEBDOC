<link rel="stylesheet" href="~/css/viewer.part.bundle.css" />
<script src="~/js/viewer.part.bundle.js"></script>
<script src="~/js/app.js/PGateInOutReport.js"></script>


<div class="right_col" role="main">

    <div class="page-title x_panel">
        <div class="title_left">
            <h3>
                P-Gate In Out Report
            </h3>
        </div>
    </div>

    <div class="clearfix"></div>

    <div class="row">
        <div class="col-md-12 col-sm-12 col-xs-12">
            <div class="x_panel">
                <div class="x_content" id="">

                    <div class="row">

                        <div class="col-md-4">
                            <label class="control-label col-md-3 col-sm-3 col-xs-12" for="phone">
                                Phone No:
                            </label>
                            <div class="col-md-9">
                                <input type="text" maxlength="12" class="form form-control" id="phone">
                            </div>
                        </div>

                        <div class="col-md-3">
                            <label class="control-label col-md-3 col-sm-3 col-xs-12" for="name">
                                Name:
                            </label>
                            <div class="col-md-9">
                                <input type="text" class="form form-control" id="name">
                            </div>
                        </div>

                        <div class="col-md-3">
                            <label class="control-label  col-md-3 col-xs-12" for="cnic">
                                CNIC:
                            </label>
                            <div class="col-md-9 col-xs-12">
                                <input type="text" maxlength="15" class="form form-control" id="cnic">
                            </div>
                        </div>
                    </div>
                    <br />
                    <div class="col-md-10>
                        @{Html.RenderPartial("_DateFilter"); }
                    </div>

                    
                </div>
            </div>
        </div>
    </div>

    <div class="row">

        <div class="col-md-12 col-sm-12 col-xs-12">
            <div class="x_panel">
                <div id="large-indicator"></div>
                <div class="x_content" id="repotdata">

                </div>
            </div>
        </div>

    </div>

</div>



<script>
    $('.select2').select2();
</script>